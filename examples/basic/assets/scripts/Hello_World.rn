pub struct Hello_World {
	name
}

impl Hello_World {
	pub fn new() {
		Hello_World {
			name: "World"
		}
	}
	pub fn build(self) {
		print(`HELLO BUILD! I'm ${self.name}!`);
		self.name = "im different!";
		require_asset("sprites/test.png");
		require_asset("sprites/test2.png");
		require_asset("sprites/hello_world.png");
	}
	pub fn start(self) {
		print("Hello World!");
	}
}
